// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Language {
  Source_ID     String  @id
  Original_Text String?
  Translation   String?
  Fuzzy_Forms   String?
  Sonetics      String?
  Entry_Type    String?
}

model Sources {
  Source_Name            String
  Source_ID              String    @id
  English_Scans          String?
  Database_Entry         String?
  File_Name              String?
  Source_of_Words        String
  Publication_Status     String?
  Type_of_Document       String?
  Collector              String?
  Indigenous_Consultants String?
  Date                   DateTime?
  Location               String?
  Language/s(Named in Source) String?
  Language(Name Regularised) String?
  Notes                  String?
}
model Boomwurrung Sources {
Source_ID String @id
Translation String?
Original String?
}
model Preliminary Root Glossaries {
Root_Spelling String?
English_Term String?
Synonyms String?
Semantic_Field_1 String?
Semantic_Field_2 String?
Word_Class(Noun/Verb) String?
Source_ID String @id
}
model Preliminary Affix Glossaries {
Root_Spelling String?
English_Term String?
Abreviation String?
Semantic_Field_1 String?
Semantic_Field_2 String?
Word_Class(Noun/Verb) String?
Source_ID String @id
}

model AIATSIS {
  Glossary_Count   Int?
  Category         String?
  Number           Int?
  Glossary         String?
  HG_Glossary_Form String?
  Semantic_Field   String?
  Speech_Part(Noun/Adjective) String?
  Other_Note       String?
  Change_Note      String?
  Notes            String?
  Scientific_Name  String?
  Kin_Designation  String?
  SW_Rec_ID        String?
  HG_Basic         String?
}
model Filtered Results {
Source String
Unique_ID String @id @unique
Entry_ID String
Glossary_Translation String?
Original String?
}
